#h-panel-container
  float left
  width 280px
  position absolute
  z-index 999
  margin-left 5px
  top 0

.h-panel
  background-color #ffffff
  opacity .95
  margin-top 5px
  border-radius 3px
  height auto
  border 1px solid #d6d6d6

.h-panel-content
  margin 5px

.h-panel-content:empty
  margin 0

.h-panel-title
  font-weight bold
  text-align left
  background-color #f0f0f0
  cursor pointer
  padding 5px 5px

  span
    float right
